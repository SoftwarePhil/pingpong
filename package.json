{
  "name": "pingpong",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-turbo": "next dev --turbopack",
    "dev-fast": "next dev --turbo",
    "dev-legacy": "NODE_OPTIONS='--no-deprecation' next dev",
    "dev-watch": "next dev --experimental-app-only",
    "build": "next build",
    "start": "next start",
    "build-run": "npm run build && npm start",
    "lint": "eslint",
    "redis:start": "docker compose up -d redis",
    "redis:stop": "docker compose down"
  },
  "dependencies": {
    "next": "15.5.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
